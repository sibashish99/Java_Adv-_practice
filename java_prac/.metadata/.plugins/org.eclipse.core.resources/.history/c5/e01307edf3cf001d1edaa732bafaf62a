package com.example.lambda;

public class RunnableApp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Runnable thread1  = () -> {
        	for(int i =0;i<11;i++) {
        		System.out.println("Value of the thread is +"i);
        		try {
        			Thread.sleep(1000);
				} catch (Exception e) {
					// TODO: handle exception
					e.printStackTrace();
				}
        	}
        };
	}

}
